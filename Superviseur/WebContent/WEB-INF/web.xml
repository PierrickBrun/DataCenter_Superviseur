<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">

  <display-name>Superviseur</display-name>
  
  <servlet>
    <description>JAX-RS Tools Generated - Do not modify</description>
    <servlet-name>S1</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jersey.config.server.provider.packages</param-name>
      <param-value>fr.ujfgrenoble.PC.services</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S1</servlet-name>
    <url-pattern>/jaxrs/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description>IHM du superviseur</description>
    <servlet-name>S2</servlet-name>
    <servlet-class>fr.ujfgrenoble.PC.ihm.SIHM</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S2</servlet-name>
    <url-pattern>/SIHM/*</url-pattern>
  </servlet-mapping>
  
  <listener>
    <listener-class>fr.ujfgrenoble.PC.utils.ContextListener</listener-class>
  </listener>

</web-app>